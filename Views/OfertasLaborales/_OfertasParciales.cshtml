
@model IEnumerable<WebApplication4.Models.OfertaLaboral>

<table class="table">
    <thead>
        <tr>
            <th>@Html.DisplayNameFor(model => model.Cargo)</th>
            <th>@Html.DisplayNameFor(model => model.Vacante)</th>
            <th>@Html.DisplayNameFor(model => model.Descripcion)</th>
            <th>@Html.DisplayNameFor(model => model.FechaLimite)</th>
            <th>@Html.DisplayNameFor(model => model.Salario)</th>
            <th>@Html.DisplayNameFor(model => model.Lugar)</th>
            <th>@Html.DisplayNameFor(model => model.IdEmpresa)</th>
            <th></th>
        </tr>
    </thead>
    <tbody>
    @foreach (var item in Model)
    {
        <tr>
            <td>@Html.DisplayFor(modelItem => item.Cargo)</td>
            <td>@Html.DisplayFor(modelItem => item.Vacante)</td>
            <td>@Html.DisplayFor(modelItem => item.Descripcion)</td>
            <td>@Html.DisplayFor(modelItem => item.FechaLimite)</td>
            <td>@Html.DisplayFor(modelItem => item.Salario)</td>
            <td>@Html.DisplayFor(modelItem => item.Lugar)</td>
            <td>@Html.DisplayFor(modelItem => item.IdEmpresa)</td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.idOfertaLaboral">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.idOfertaLaboral">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.idOfertaLaboral">Delete</a>
            </td>
        </tr>
    }
    </tbody>
</table>

